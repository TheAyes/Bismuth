{
  "name": "my-qwik-empty-starter",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build-storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "latest",
    "@builder.io/qwik-city": "latest",
    "@chromatic-com/storybook": "1.2.3",
    "@storybook/addon-essentials": "^8.0.0-rc.1",
    "@storybook/addon-interactions": "8.0.0-rc.1",
    "@storybook/addon-links": "^8.0.0-rc.1",
    "@storybook/blocks": "^8.0.0-rc.1",
    "@storybook/builder-vite": "^8.0.0-rc.1",
    "@storybook/html": "^8.0.0-rc.1",
    "@storybook/html-vite": "^8.0.0-rc.1",
    "@storybook/testing-library": "0.2.2",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-plugin-qwik": "^1.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "prettier": "latest",
    "storybook": "^8.0.0-rc.1",
    "storybook-framework-qwik": "^0.3.0",
    "typescript": "5.3.3",
    "undici": "*",
    "vite": "latest",
    "vite-tsconfig-paths": "latest"
  }
}
